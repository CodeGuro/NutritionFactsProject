<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
	<managed-bean>
		<managed-bean-name>regBean</managed-bean-name>
		<managed-bean-class>beans.RegistrationBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<description>A login bean that handles Login.xhtml</description>
		<managed-bean-name>loginBean</managed-bean-name>
		<managed-bean-class>beans.LoginBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<description>This is the admin session managed bean</description>
		<managed-bean-name>adminSession</managed-bean-name>
		<managed-bean-class>beans.AdminSession</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>adminFoodsPageViewBean</managed-bean-name>
		<managed-bean-class>beans.AdminFoodsPageViewBean</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
		<managed-property>
			<property-name>sessionBean</property-name>
			<property-class>beans.AdminSession</property-class>
			<value>#{adminSession}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>adminIngredientsPageViewBean</managed-bean-name>
		<managed-bean-class>beans.AdminIngredientsPageViewBean</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
		<managed-property>
			<property-name>sessionBean</property-name>
			<property-class>beans.AdminSession</property-class>
			<value>#{adminSession}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>adminNutritionBean</managed-bean-name>
		<managed-bean-class>beans.AdminNutritionalFactsPageViewBean</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
		<managed-property>
			<property-name>sessionBean</property-name>
			<property-class>beans.AdminSession</property-class>
			<value>#{adminSession}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>adminMenusPageViewBean</managed-bean-name>
		<managed-bean-class>beans.AdminMenusPageViewBean</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
		<managed-property>
			<property-name>adminSession</property-name>
			<property-class>beans.AdminSession</property-class>
			<value>#{adminSession}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>clientSession</managed-bean-name>
		<managed-bean-class>beans.ClientSession</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<validator>
	<display-name>Password Validator</display-name>
	<validator-id>passwordValidator</validator-id>
	<validator-class>beans.PasswordValidator</validator-class>
	</validator>
	<validator>
		<display-name>Email Validator</display-name>
		<validator-id>emailValidator</validator-id>
		<validator-class>resources.EmailValidator</validator-class>
	</validator>
	<navigation-rule>
		<display-name>Registration.xhtml</display-name>
		<from-view-id>/Registration.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{regBean.addUser}</from-action>
			<from-outcome>success</from-outcome>
			<to-view-id>/RegConfirmation.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Registration.xhtml</display-name>
		<from-view-id>/Registration.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{regBean.addUser}</from-action>
			<from-outcome>failure</from-outcome>
			<to-view-id>/Registration.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Login.xhtml</display-name>
		<from-view-id>/Login.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{loginBean.login}</from-action>
			<from-outcome>failure</from-outcome>
			<to-view-id>/Login.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Login.xhtml</display-name>
		<from-view-id>/Login.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{loginBean.login}</from-action>
			<from-outcome>success</from-outcome>
			<to-view-id>/AdminPage.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>AdminPage.xhtml</display-name>
		<from-view-id>/AdminPage.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>EditMenus</from-outcome>
			<to-view-id>/MenusPage.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>AdminPage.xhtml</display-name>
		<from-view-id>/AdminPage.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>EditFoods</from-outcome>
			<to-view-id>/FoodsPage.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>AdminPage.xhtml</display-name>
		<from-view-id>/AdminPage.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>EditIngredients</from-outcome>
			<to-view-id>/IngredientsPage.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>MenusPage.xhtml</display-name>
		<from-view-id>/MenusPage.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{adminMenusPageViewBean.createNewMenu}</from-action>
			<from-outcome>createSuccess</from-outcome>
			<to-view-id>/MenusPage.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>MenusPage.xhtml</display-name>
		<from-view-id>/MenusPage.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{adminMenusPageViewBean.deleteMenu}</from-action>
			<from-outcome>delSuccess</from-outcome>
			<to-view-id>/MenusPage.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>MenusPage.xhtml</display-name>
		<from-view-id>/MenusPage.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{adminMenusPageViewBean.editMenu}</from-action>
			<from-outcome>goToEditPage</from-outcome>
			<to-view-id>/EditMenusPage.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>FoodsPage.xhtml</display-name>
		<from-view-id>/FoodsPage.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{adminFoodsPageViewBean.prepareNewIngredient}</from-action>
			<from-outcome>goToEditPage</from-outcome>
			<to-view-id>/EditFoodsPage.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>FoodsPage.xhtml</display-name>
		<from-view-id>/FoodsPage.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{adminFoodsPageViewBean.createFood}</from-action>
			<from-outcome>createSuccess</from-outcome>
			<to-view-id>/FoodsPage.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>FoodsPage.xhtml</display-name>
		<from-view-id>/FoodsPage.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{adminFoodsPageViewBean.deleteFood}</from-action>
			<from-outcome>delSuccess</from-outcome>
			<to-view-id>/FoodsPage.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>EditFoodsPage.xhtml</display-name>
		<from-view-id>/EditFoodsPage.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>Return</from-outcome>
			<to-view-id>/FoodsPage.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>FoodsPage.xhtml</display-name>
		<from-view-id>/FoodsPage.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>Return</from-outcome>
			<to-view-id>/AdminPage.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>IngredientsPage.xhtml</display-name>
		<from-view-id>/IngredientsPage.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>Return</from-outcome>
			<to-view-id>/AdminPage.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>MenusPage.xhtml</display-name>
		<from-view-id>/MenusPage.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>Return</from-outcome>
			<to-view-id>/AdminPage.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>IngredientsPage.xhtml</display-name>
		<from-view-id>/IngredientsPage.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{adminIngredientsPageViewBean.editNutritions}</from-action>
			<from-outcome>goToEditPage</from-outcome>
			<to-view-id>/EditNutritionalPage.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>EditNutritionalPage.xhtml</display-name>
		<from-view-id>/EditNutritionalPage.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>Return</from-outcome>
			<to-view-id>/IngredientsPage.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>AdminPage.xhtml</display-name>
		<from-view-id>/AdminPage.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>Logout</from-outcome>
			<to-view-id>/Login.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>EditMenusPage.xhtml</display-name>
		<from-view-id>/EditMenusPage.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>Return</from-outcome>
			<to-view-id>/MenusPage.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>MainPage.xhtml</display-name>
		<from-view-id>/MainPage.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{clientSession.setSelectedMenu(menuItem)}</from-action>
			<from-outcome>goToFoodCustomizationPage</from-outcome>
			<to-view-id>/FoodCustomizationPage.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>FoodCustomizationPage.xhtml</display-name>
		<from-view-id>/FoodCustomizationPage.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>goToMain</from-outcome>
			<to-view-id>/MainPage.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>FoodCustomizationPage.xhtml</display-name>
		<from-view-id>/FoodCustomizationPage.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{clientSession.setSelectedFood(food)}</from-action>
			<from-outcome>goToIngredientCustomizationPage</from-outcome>
			<to-view-id>/IngredientsCustomizationPage.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>IngredientsCustomizationPage.xhtml</display-name>
		<from-view-id>/IngredientsCustomizationPage.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>goToMain</from-outcome>
			<to-view-id>/MainPage.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>IngredientsCustomizationPage.xhtml</display-name>
		<from-view-id>/IngredientsCustomizationPage.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>goBack</from-outcome>
			<to-view-id>/FoodCustomizationPage.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>FoodCustomizationPage.xhtml</display-name>
		<from-view-id>/FoodCustomizationPage.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>GoToFinalizationPage</from-outcome>
			<to-view-id>/FinalizationPage.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>FinalizationPage.xhtml</display-name>
		<from-view-id>/FinalizationPage.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>goBack</from-outcome>
			<to-view-id>/FoodCustomizationPage.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>FinalizationPage.xhtml</display-name>
		<from-view-id>/FinalizationPage.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>goToMain</from-outcome>
			<to-view-id>/MainPage.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>FinalizationPage.xhtml</display-name>
		<from-view-id>/FinalizationPage.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>goToNutritionPage</from-outcome>
			<to-view-id>/NutritionPage.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>NutritionPage.xhtml</display-name>
		<from-view-id>/NutritionPage.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>goToMain</from-outcome>
			<to-view-id>/MainPage.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>
